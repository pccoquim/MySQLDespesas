<Window x:Class="GestaoDom.Frm_900000_Help"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestaoDom"
        mc:Ignorable="d"
        Title="Help" Height="150" Width="200">
    <Grid>
        <Label Name="Lbl_Version" Content="Version:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10, 10, 0, 0" Height="30" Width="50" />
        <Label Name="Lbl_VersionN" Content="{Binding Version}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="70, 10, 0, 0" Height="30" Width="50" />
    </Grid>
</Window>
