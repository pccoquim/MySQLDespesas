<Window x:Class="GestaoDom.Frm_0000_Status"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestaoDom"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Criação de estados" Height="130" Width="280">
    <Grid>
        <Label Name="lbl_status" Content="Estado" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10, 10, 0, 0" Width="70" />
        <TextBox Name="txt_UserID" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="80,15,0,0" IsReadOnly="False" TextWrapping="Wrap" Text="" Height="20" Width="140" />
        <Button Name="btn_Save" Content="Gravar" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="80, 55, 0,0" Width="60" Height="20" Click="Btn_Save_Click"/>
        <Button Name="btn_Close" Content="Fechar" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="160, 55, 0,0" Width="60" Height="20" Click="Btn_Close_Click"/>
    </Grid>
</Window>
